<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--  开关-->
    <declare-styleable name="ToggleButton">
        <attr name="onColor" format="color|reference"/>
        <attr name="offColor" format="color|reference"/>
    </declare-styleable>
    <!--  圆形图像-->
    <declare-styleable name="roundedimageview">
        <attr name="border_thickness" format="dimension" />
        <attr name="border_inside_color" format="color" />
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>

    <!--  饼图-->
    <declare-styleable name="PieChartView">
        <attr name="circleRadius" format="dimension" />
        <attr name="textSize" format="dimension" />
        <attr name="exampleDrawable" format="color|reference" />
    </declare-styleable>

    <!--  搜索框-->
    <declare-styleable name="search_view">
        <attr name="hint" format="string" />
        <attr name="show_search_button" format="boolean" />
    </declare-styleable>
</resources>
